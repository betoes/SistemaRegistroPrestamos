<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.cell.PropertyValueFactory ?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller ="gui.PantallaLicenciaControlador">
	   <children>
      <AnchorPane layoutX="-1.0" prefHeight="589.0" prefWidth="936.0">
         <children>
            <TableView fx:id="tbLicencia" layoutX="5.0" layoutY="19.0" prefHeight="540.0" prefWidth="749.0">
               <columns>
                <TableColumn prefWidth="36.0" text="idLicencia">
                	<cellValueFactory>
                		<PropertyValueFactory property="idLicencia" />
                	</cellValueFactory> 
                </TableColumn>
                <TableColumn prefWidth="113.0" text="NumeroLicencias">
                	<cellValueFactory>
                		<PropertyValueFactory property="numeroLicencias" />
                	</cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="96.0" text="FechaInicio">
                	<cellValueFactory>
                		<PropertyValueFactory property="fechaInicio" />
                	</cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="72.0" text="FechaFin">
      					<cellValueFactory>
                		<PropertyValueFactory property="fechaFin" />
                	</cellValueFactory>                
                </TableColumn>
                <TableColumn prefWidth="145.0" text="Clave">
      					<cellValueFactory>
                		<PropertyValueFactory property="clave" />
                	</cellValueFactory>                
                </TableColumn>
                <TableColumn minWidth="7.0" prefWidth="87.0" text="Proveedor">
      					<cellValueFactory>
                		<PropertyValueFactory property="proveedor" />
                	</cellValueFactory>                
                </TableColumn>
                <TableColumn minWidth="6.0" prefWidth="91.0" text="Caracter">
      					<cellValueFactory>
                		<PropertyValueFactory property="caracter" />
                	</cellValueFactory>                
                </TableColumn>
                <TableColumn prefWidth="108.0" text="Tipo">
      					<cellValueFactory>
                		<PropertyValueFactory property="tipoLicenciamiento" />
                	</cellValueFactory>                
                </TableColumn>
              </columns>
            </TableView>
            <Button fx:id="bagregar" layoutX="760.0" layoutY="73.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="116.0" text="Agregar" onAction="#cargarPantallaAgregarLicencia" />
            <Button fx:id="beliminar" layoutX="760.0" layoutY="142.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="116.0" text="Eliminar " onAction="#cargarPantallaElimanarLicencia"/>
            <Button fx:id="bmodificar" layoutX="760.0" layoutY="216.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="116.0" text="Modificar " onAction="#cargarPantallaModificarLicencia" />
            <Button fx:id="bbuscar" layoutX="760.0" layoutY="284.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="116.0" text="Buscar" onAction="#cargarPantallaBuscarLicencia" />
            <Button fx:id="bsalir" layoutX="760.0" layoutY="355.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="116.0" text="Salir" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>

